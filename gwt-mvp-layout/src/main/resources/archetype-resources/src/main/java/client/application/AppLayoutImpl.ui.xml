#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:style>
        .northPanel {
            height: 100%;
            width: 100%;
            border: 1px solid gray;
        }

        .southPanel {
            height: 100%;
            width: 100%;
            border: 1px solid gray;
        }

        .westPanel {
            height: 100%;
            width: 100%;
            border: 1px solid gray;
        }

        .eastPanel {
            height: 100%;
            width: 100%;
            border: 1px solid gray;
        }

        .centerPanel {
            height: 100%;
            width: 100%;
            border: 1px solid gray;
        }
    </ui:style>

    <g:DockLayoutPanel unit='EM'>
        <g:center type="">
            <g:SimpleLayoutPanel ui:field="centerPanel"
                                 addStyleNames="{style.centerPanel}"/>
        </g:center>
        <g:east size="10">
            <g:SimpleLayoutPanel ui:field="eastPanel" addStyleNames="{style.eastPanel}"/>
        </g:east>
        <g:west size="10">
            <g:SimpleLayoutPanel ui:field="westPanel" addStyleNames="{style.westPanel}"/>
        </g:west>
        <g:north size="10">
            <g:SimpleLayoutPanel ui:field="northPanel" addStyleNames="{style.northPanel}"/>
        </g:north>
        <g:south size="10">
            <g:SimpleLayoutPanel ui:field="southPanel" addStyleNames="{style.southPanel}"/>
        </g:south>
    </g:DockLayoutPanel>
</ui:UiBinder> 